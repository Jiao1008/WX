/**index.wxss**/
.indexContainer {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: rgb(187, 187, 187, .7);

}

.avatarUrl {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin-top: 100rpx;
}

.userName {
  font-size: 32rpx;
  margin: 100rpx auto;
}

.goStudy {
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border: 2px dashed #fff;
  border-radius: 10rpx;
}